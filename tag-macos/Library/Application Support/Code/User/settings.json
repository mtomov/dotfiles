{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[erb]": {
    "editor.defaultFormatter": "elia.erb-formatter",
    "editor.autoClosingBrackets": "never"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "breadcrumbs.enabled": true,
  "css.customData": [".vscode/css_custom_data.json"],
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "Hasklig, Inconsolata, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.minimap.scale": 2,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderLineHighlight": "gutter",
  "editor.suggest.localityBonus": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.wordWrap": "on",
  "emmet.excludeLanguages": ["markdown", "typescript", "typescriptreact"],
  "emmet.includeLanguages": {
    "erb": "html"
  },
  "emmet.showAbbreviationSuggestions": false,
  "emmet.triggerExpansionOnTab": true,
  "emojisense.emojiDecoratorsEnabled": true,
  "emojisense.languages": {
    "git-commit": true,
    "json": true,
    "markdown": true,
    "plaintext": true
  },
  "emojisense.markupCompletionsEnabled": false,
  "emojisense.showOnColon": true,
  "emojisense.unicodeCompletionsEnabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.casks": "ruby",
    "*.config": "yaml",
    "*.erb": "erb",
    ".aliases": "shellscript",
    ".irbrc_rails": "ruby",
    ".xbindkeysrc": "shellscript",
    "pre-push": "shellscript"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/*.tfstate": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/deps/": true,
    "**/node_modules": true,
    "yarn.lock": false
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.autoStash": true,
  "git.autofetch": true,
  "git.confirmForcePush": false,
  "git.rebaseWhenSync": true,
  "git.supportCancellation": true,
  "github.customPullRequestDescription": "gitEditor",
  "github.preferedMergeMethod": "rebase",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.telemetry.enabled": false,
  "gitlens.blame.highlight.locations": ["gutter", "overview"],
  "gitlens.changes.locations": ["gutter", "overview"],
  "gitlens.codeLens.scopes": ["document", "blocks"],
  "gitlens.views.repositories.files.layout": "list",
  "gitlens.views.repositories.showBranches": false,
  "gitlens.views.repositories.showCommits": false,
  "gitlens.views.repositories.showContributors": false,
  "gitlens.views.repositories.showRemotes": false,
  "gitlens.views.repositories.showStashes": false,
  "gitlens.views.repositories.showTags": false,
  "html.format.templating": true,
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "multiCommand.commands": [
    {
      "command": "multiCommand.sameKeyValue",
      "sequence": [
        "cursorWordLeftSelect",
        "editor.action.clipboardCopyAction",
        "cursorRight",
        {
          "args": {
            "snippet": ": $CLIPBOARD, $0"
          },
          "command": "editor.action.insertSnippet"
        }
      ],
      "languages": ["ruby"]
    },
    {
      "command": "multiCommand.debug",
      "sequence": [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "args": {
            "snippet": "d { $CLIPBOARD }\n$0"
          },
          "command": "editor.action.insertSnippet"
        }
      ],
      "languages": ["ruby"]
    },
    {
      "command": "multiCommand.let",
      "sequence": [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineBefore",
        {
          "args": {
            "snippet": "let(:$CLIPBOARD) { $0 }"
          },
          "command": "editor.action.insertSnippet"
        }
      ],
      "languages": ["ruby"]
    }
  ],
  "prettier.requireConfig": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "remote.SSH.configFile": "~/.ssh/config",
  "ruby.specCommand": "bin/rspec",
  "ruby.specSaveFile": true,
  "sass.format.convert": false,
  "scm.diffDecorations": "gutter",
  "scss.validate": false,
  "search.exclude": {
    ".yarn/releases": true,
    "**/build/contracts/**": true,
    "**/deps/": true,
    "**/index.android.bundle": true
  },
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "security.workspace.trust.untrustedFiles": "open",
  "solargraph.autoformat": true,
  "solargraph.formatting": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.includeLanguages": {
    "slim": "html",
    "slim.html": "html"
  },
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.commandsToSkipShell": [
    "extension.runLastSpec",
    "workbench.action.toggleSidebarVisibility"
  ],
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.2,
  "typescript.format.enable": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vscodeWorkspaceSwitcher.paths": ["~/work/*", "~/beam/*", "~/projects/*"],
  "vscodeWorkspaceSwitcher.showInExplorer": false,
  "vscodeWorkspaceSwitcher.showTreeView": false,
  "vsicons.dontShowNewVersionMessage": true,
  // "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}${separator}${appName}",
  // "window.titleBarStyle": "custom",
  // "workbench.colorCustomizations": {
  // "statusBar.background": "#1A1A1A"
  // "statusBar.noFolderBackground": "#212121"
  // "statusBar.debuggingBackground": "#263238",
  // "tab.activeBorder": "#ff0a7c",
  // "tab.activeBackground": "#ff0a7c0a"
  // },
  "workbench.editor.labelFormat": "short",
  "workbench.editor.limit.enabled": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "yaml.format.enable": false,
  "gitlens.statusBar.enabled": false,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false,
    "ruby": true,
    "haml": false,
    "jsonc": true,
    "git-commit": false,
    "slim": false
  },
  "workbench.editor.revealIfOpen": true,
  "explorer.autoReveal": "focusNoScroll",
  "workbench.editor.scrollToSwitchTabs": true,
  "gitlens.views.showRelativeDateMarkers": false,
  "gitlens.views.commits.avatars": false,
  "gitlens.views.commits.showBranchComparison": false,
  "workbench.commandPalette.preserveInput": true,
  "gitlens.gitCommands.sortBy": "name",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "scm.repositories.visible": 1,
  "githubPullRequests.pullBranch": "never",
  "notebook.output.textLineLimit": 90,
  "python.defaultInterpreterPath": "/Users/martin/.local/share/virtualenvs/embedding_extractor-s0Qj4iay",
  "gitlens.mode.active": "zen",
  "editor.guides.bracketPairs": true,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vue.inlayHints.optionsWrapper": false,
  "editor.unicodeHighlight.allowedCharacters": {
    "â€˜": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vscode-erb-beautify.keepBlankLines": 1,
  "erb-formatter.commandPath": "erb-format",
  "erb-formatter.lineLength": "100",
  "nightswitch.themeNight": "Default Dark Modern",
  "nightswitch.themeDay": "Default Light Modern",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp"
  },
  "rubyLsp.enableExperimentalFeatures": true,
  "solargraph.completion": false,
  "solargraph.symbols": false,
  // "nightswitch.disableAutoSwitchNotifications": true,
  "nightswitch.sunset": "8:00 PM",
  "nightswitch.location": "51,0",
  "diffEditor.experimental.useVersion2": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "workbench.colorTheme": "Default Dark Modern",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.accessibilitySupport": "off",
  "nightswitch.disableAutoSwitchNotifications": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
